FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 0.1;

boundaryField
{
    freeSurf
    {
        type            kqRWallFunction;
        value           uniform 0.1;
    }
    meltWall
    {
        type            kqRWallFunction;
        value           uniform 0.1;
    }
    crysInter
    {
        type            kqRWallFunction;
        value           uniform 0.1;
    }
}


